<script>
    var u = window.navigator.userAgent;
    function clickIosOrDown (){
      $('#toastList').show()
      setTimeout(data=>{
        $('#toastList').hide()
      },2000)
    }
    //检查是否运行在Android设备
    function checkIsAndroid(){
        return u.indexOf('Android')>-1 || u.indexOf('Adr')>-1;
    }

    //检查是否运行在iOS设备
    function checkIsIOS(){
        return !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/);
    }

    //检查设备平台（android/ios/pc）
    function checkDevicePlatform(){
        if(checkIsAndroid()) return "android";
        else if(checkIsIOS()) return "ios";
        return "pc";
    }
    /*点击显示大图*/
    function clickImg (e){
      document.getElementById("wxcodebgimg").style.display = 'none'
      document.getElementById("wxcodebg").style.display = 'none'
      document.getElementById("bigImgimg").style.display = 'block'
      document.getElementById("bigImgimg").setAttribute('src', e.srcElement.currentSrc)
      document.getElementById("bigImg").style.display = 'block'
      document.getElementById("bigImg").style.position = 'fixed'
      document.getElementById("bigImg").style.top = '10vh'
      document.getElementById("bigImg").style.left = '10vw'
    }
    /*点击显示微信二维码*/
    function clickWxCode (){
      document.getElementById("wxcodebg").style.display = 'block'
      document.getElementById("wxcodebgimg").style.display = 'block'
      document.getElementById("bigImgimg").style.display = 'none'
      document.getElementById("wxcodebgimg").setAttribute('src', 'images/wxcode.png')
      document.getElementById("bigImg").style.display = 'block'
      document.getElementById("bigImg").style.position = 'fixed'
    }
    function clickBigImg(){
        document.getElementById("bigImg").style.display = 'none'
        document.getElementById("wxcodebg").style.display = 'none'
    }
    //打开aihelp
    function openAIHelp(){
        var conf = {
            style: 2,
            domain: 'https://gamewinner.aihelp.net/webchat/#',
            appId: 'Gamewinner_pf_818667d93c7c4cd7974bd40d6b8c0e4f',
            appKey: 'GAMEWINNER_app_68868b87956643f58f1d6e554f9e2643',
            appName: 'Game Help',
            language: 'en'
        };
        elvah5.init(conf);
    }
    var plat = checkDevicePlatform();
    //设置pc下载地址
    var dlUrls = {
        ios: 'https://apps.apple.com/vn/app/id6443444765',
        android: 'https://gwgamepkg.oss-cn-shanghai.aliyuncs.com/csjs2/csjs2lycs-release-v1.0.0-202303012300.apk',
        wx:  null,
        pc: 'https://yxdserver.52177.com/api/online/AQEHU1HMRGLM?#/enter_game',

        telegram: 'https://t.me/idlefantasy_bot',
        whitePaper: 'docs/whitePaper.pdf',
        reddit: 'https://www.reddit.com/r/IdleFantasy_x',
        discord: 'https://discord.com/channels/1298850233257824316/1298851346388221992',
        twitter: 'https://x.com/idlefantasy_x',
        telegramGrop: 'https://t.me/idlefantasy_bot' 
    }
    var url = dlUrls[plat];
    /*res == 1 点击 底部跳转html res==2中部按钮点击跳转  为 pc 点击跳转到指定html*/
    function clickLink(event,res,type){
        event && (event.cancelBubble = true)
        let obj = { /*商店地址*/
            ios: 'https://apps.apple.com/vn/app/id6443444765',
            android: 'https://gwgamepkg.oss-cn-shanghai.aliyuncs.com/csjs2/csjs2lycs-release-v1.0.0-202303012300.apk',
            wx: null,
            pc: 'https://yxdserver.52177.com/api/online/AQEHU1HMRGLM?#/enter_game',

            telegram: 'https://t.me/idlefantasy_bot',
            whitePaper: 'docs/whitePaper.pdf',
            reddit: 'https://www.reddit.com/r/IdleFantasy_x',
            discord: 'https://discord.com/channels/1298850233257824316/1298851346388221992',
            twitter: 'https://x.com/idlefantasy_x',
            telegramGrop: 'https://t.me/idlefantasy_bot'
        }
        if(type == 'wx' && plat == 'pc'){
          clickWxCode()
          return
        }

        if(type == 'pc-ios'){ /*点击下载  mac 跳转到苹果商城 pc跳转到h5**/
          type = navigator.platform.indexOf('Mac') > -1 ? 'ios' : 'android'
          url = plat == 'pc'  ? dlUrls[type]  : obj[plat] ; /*pc 端 用type 移动端用plat*/
        }else{ /*点击其它按钮*/
          url = plat == 'pc'  ? dlUrls[type]  : obj[type] ;
        }
        window.open(url)
    }

    let ua = navigator.userAgent.toLowerCase()
    function clickQQ(){ /*联系QQ客服*/
      if(plat == 'pc'){
        window.open('tencent://AddContact/?fromId=45&fromSubId=1&subcmd=all&uin=2355488915')
      }else if ((ua.indexOf('mqqbrowser')> -1 && ua.indexOf(" qq") < 0) && plat == 'ios'){/* 微信内置浏览器跳转  QQ浏览器跳转*/
        window.open('mqqwpa://im/chat?chat_type=wpa&uin=2355488915&version=1&src_type=web&web_src=https://www.idlegog2.com/cn')
      }else if (ua.match(/MicroMessenger/i) == 'micromessenger'){
        $("#open").show()
        $('#open').click(data=>{
          $("#open").hide()
        })
        return
      }else{
        window.open('mqqapi://card/show_pslcard?src_type=internal&version=1&uin=2355488915&card_type=person&source=sharecard')
      }
      history.go(0)
    }
    //设置背景图
    var imgUrls = ['bg_01.jpg', 'bg_02.jpg', 'bg_03.jpg'];
    var idx = Math.round(Math.random() * (imgUrls.length-1));
    var imgDiv = document.getElementById('bgDiv');
    imgDiv.style.backgroundImage = "url('./images/" + imgUrls[idx] + "')";
</script>

